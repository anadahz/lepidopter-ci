#!/usr/bin/env sh
qemu-system-arm \
    -M versatilepb \
    -cpu arm1136-r2 \
    -kernel ${QEMU_KERNEL} \
    --nographic \
    -drive file=${IMAGE},format=raw \
    -append "root=/dev/sda2 rootfstype=ext4 rw console=ttyAMA0" \
    -net nic -net user,hostfwd=tcp::${SSH_PORT}-:22,hostfwd=tcp::8080-:80
